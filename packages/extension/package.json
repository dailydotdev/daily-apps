{
  "name": "@daily/extension",
  "version": "2.27.3",
  "private": true,
  "scripts": {
    "serve": "cross-env TARGET=development yarn manifest && vue-cli-service build --mode development --watch",
    "build": "yarn manifest && vue-cli-service build",
    "build:chrome": "cross-env TARGET=chrome yarn build --mode chrome",
    "build:firefox": "cross-env TARGET=firefox yarn build --mode firefox",
    "build:all": "yarn build:chrome && yarn build:firefox && yarn zip:src",
    "test": "yarn lint && vue-cli-service test:unit",
    "lint": "vue-cli-service lint ./src/**/*.js ./src/**/*.vue ./__tests__/**/*.js",
    "test:unit": "vue-cli-service test:unit",
    "test:ci": "yarn lint && cross-env JEST_JUNIT_OUTPUT=../../test-results/extension/junit.xml vue-cli-service test:unit --ci --reporters=jest-junit && bundlesize",
    "zip:src": "node build/zip-src.js",
    "manifest": "node build/manifest.js"
  },
  "dependencies": {
    "@daily/components": "^0.2.1",
    "@daily/services": "^0.1.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "apollo-link-persisted-queries": "^0.2.2",
    "apollo-link-ws": "^1.0.20",
    "apollo-utilities": "^1.3.4",
    "core-js": "^2",
    "date-fns": "^2.16.1",
    "dompurify": "^2.1.1",
    "fast-deep-equal": "^3.1.1",
    "focus-visible": "^4.1.5",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "lazysizes": "^5.2.2",
    "lerna": "^3.4.3",
    "subscriptions-transport-ws": "^0.9.18",
    "v-tooltip": "^2.0.3",
    "vue": "^2.5.17",
    "vue-apollo": "^3.0.3",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1",
    "webextension-polyfill": "^0.4.0",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.4.2",
    "@vue/babel-preset-app": "^3.11",
    "@vue/cli-plugin-babel": "^3.11",
    "@vue/cli-plugin-eslint": "^3.2.1",
    "@vue/cli-plugin-unit-jest": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/preload-webpack-plugin": "^1.1.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "archiver": "^3.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-graphql-tag": "^2.5.0",
    "browserslist": "^4.7.0",
    "bundlesize": "^0.18.0",
    "caniuse-lite": "^1.0.30001066",
    "chalk": "^2.4.2",
    "deepmerge": "^3.2.0",
    "eslint": "^5.8.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "imports-loader": "^0.8.0",
    "jest": "^23.6.0",
    "jest-junit": "^6.4.0",
    "mock-apollo-client": "^0.4.0",
    "mockdate": "^3.0.2",
    "ora": "^3.3.0",
    "postcss-custom-media": "^7.0.7",
    "postcss-extend": "^1.0.5",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^4.1.2",
    "vue-cli-plugin-browser-extension": "^0.14.0",
    "vue-template-compiler": "^2.5.17",
    "workbox-webpack-plugin": "^4.1.1"
  },
  "resolutions": {
    "@babel/parser": "7.7.5"
  },
  "browserslist": [
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/[^.]*.(js|jsx|ts|tsx)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testURL": "http://localhost/"
  }
}
