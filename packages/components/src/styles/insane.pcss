.insane {
    display: flex;
    width: 100%;
    height: 72px;
    padding: 24px;
    flex-direction: row;
    background: var(--theme-background-highlight);
    align-items: center;
    border-bottom: 1px solid var(--theme-background-primary);
    overflow: hidden;
}

.insane__link {
    flex: 1;
    text-decoration: none;
}

.insane__title {
    color: var(--theme-primary);
}
